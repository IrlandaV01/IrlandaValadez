module Memoria_ROM#(
    parameter AW = 8,   
    parameter nAddr = 256,   
    parameter DW = 9)
    ( 
    input  [AW-1:0] addr,
    output reg [DW-1:0] dato
    );
    //////memoria y parametro del fichero contenedor///
reg [(DW-1):0]mem[0:(nAddr-1)];
    
    initial
        begin
            $readmemb("ROM.memoria",mem);
        end    
    
    always @(addr) begin
        dato<=mem[addr];
    end
endmodule
